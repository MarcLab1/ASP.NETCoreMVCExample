@{
    ViewData["Title"] = "Strongly typed @model directive";
}

@model Student
<!--
a Student object is passed as a strongly typed object to this view
    at model Student
    is translated into
    Student model

-->

@Model.id <br />
@Model.fName <br />
@Model.lName <br /> <br />

@Html.DisplayFor(model => model.id)
@Html.DisplayFor(model => model.fName)
@Html.DisplayFor(model => model.lName)
